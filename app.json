{
    "expo": {
        "name": "Trix",
        "version": "1.1.2",
        "description": "Trix is a new kind of camera app that helps users prevent facial recognition and other unwanted use of their data",
        "orientation": "portrait",
        "privacy": "public",
        "icon": "./assets/app.png",
        "primaryColor": "#0d2129",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#0d2129"
        },
        "packagerOpts": {},
        "androidStatusBar": {
            "backgroundColor": "#0d2129",
            "barStyle": "dark-content"
        },
        "ios": {
            "bundleIdentifier": "co.trix.trixapp",
            "buildNumber": "1.1.2",
            "supportsTablet": true
        },
        "android": {
            "package": "co.trix.trixapp",
            "permissions": ["CAMERA", "CAMERA_ROLL", "MANAGE_DOCUMENTS", "INTERNET", "WRITE_EXTERNAL_STORAGE"],
            "versionCode": 112,
            "adaptiveIcon": {
                "backgroundColor": "#0d2129",
                "foregroundImage": "./assets/app_icon_android.png"
            }
        },
        "assetBundlePatterns": ["assets/*"],
        "slug": "trix-app",
        "platforms": ["android", "ios"]
    }
}
